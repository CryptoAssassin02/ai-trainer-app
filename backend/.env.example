   # Server Configuration
   NODE_ENV=development
   PORT=3001

   # Supabase
   SUPABASE_URL=https://your-project-ref.supabase.co
   SUPABASE_PROJECT_REF=your-project-ref
   SUPABASE_ANON_KEY=your-anon-key
   SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
   DATABASE_PASSWORD=your-database-password

   # Supabase Direct Connection Components
   DB_HOST=db.your-project-ref.supabase.co
   DB_PORT=5432
   DB_NAME=postgres
   DB_USER=postgres

   # Pooler Connection Components
   POOLER_HOST=aws-0-region.pooler.supabase.com
   POOLER_SESSION_PORT=5432
   POOLER_TRANSACTION_PORT=6543
   POOLER_USER=postgres.your-project-ref

   # Complete Connection Strings
   DATABASE_URL=postgresql://postgres:password@db.your-project-ref.supabase.co:5432/postgres
   DATABASE_URL_SERVICE_ROLE=postgresql://postgres:service-role-key@db.your-project-ref.supabase.co:5432/postgres
   DATABASE_URL_POOLER_SESSION=postgresql://postgres.your-project-ref:service-role-key@aws-0-region.pooler.supabase.com:5432/postgres
   DATABASE_URL_POOLER_TRANSACTION=postgresql://postgres.your-project-ref:service-role-key@aws-0-region.pooler.supabase.com:6543/postgres

   # SSL Configuration (only use NODE_TLS_REJECT_UNAUTHORIZED=0 for testing)
   NODE_TLS_REJECT_UNAUTHORIZED=0
   SSL_MODE=require

   # Authentication
   JWT_SECRET=your-jwt-secret-32-chars-minimum
   JWT_REFRESH_SECRET=your-refresh-secret
   JWT_EXPIRY=24h
   REFRESH_TOKEN_EXPIRY=7d

   # Rate Limiting
   RATE_LIMIT_WINDOW_MS=60000
   RATE_LIMIT_MAX=100
   WORKOUT_GEN_RATE_LIMIT=10

   # External services
   OPENAI_API_KEY=your-openai-api-key
   PERPLEXITY_API_KEY=your-perplexity-api-key

   # CORS
   CORS_ORIGIN=http://localhost:3000

   # Logging
   LOG_LEVEL=info

   # Email Notifications (optional for development)
   SMTP_HOST=smtp.example.com
   SMTP_PORT=587
   SMTP_USER=your_smtp_username
   SMTP_PASSWORD=your_smtp_password
   SMTP_FROM=noreply@trainer-app.com

   # Push Notifications (optional for development)
   FIREBASE_API_KEY=your_firebase_api_key
   FIREBASE_PROJECT_ID=your_firebase_project_id

   # Analytics Settings (optional for development)
   ANALYTICS_ENABLED=true
   ANALYTICS_PROVIDER=posthog
   ANALYTICS_API_KEY=your_analytics_api_key